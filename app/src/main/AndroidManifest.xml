<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.live.learningapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MatricesMenu" />


        <activity android:name=".MainLagrangeActivity"></activity>
        <activity
            android:name=".MainNewtonActivity"
            android:parentActivityName=".MainNewtonActivity"></activity>

        <activity android:name=".DataLagrangeActivity" />
        <activity
            android:name=".AfficheLagrangeActivity"
            android:parentActivityName=".MainLagrangeActivity" />
        <activity
            android:name=".DataNewtonActivity"
            android:parentActivityName=".MainNewtonActivity" />
        <activity
            android:name=".AfficheNewtonActivity"
            android:parentActivityName=".DataNewtonActivity" />
        <activity
            android:name=".Help"
            android:label="@string/title_activity_help"
            android:theme="@style/AppTheme" />
        <activity android:name=".About_Us" />
        <activity android:name=".Documentation"></activity>
    </application>

</manifest>